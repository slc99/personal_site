    "                            <option value=\"1\">⚀</option>\n",
    "                            <option value=\"2\">⚁</option>\n",
    "                            <option value=\"3\">⚂</option>\n",
    "                            <option value=\"4\">⚃</option>\n",
    "                            <option value=\"5\">⚄</option>\n",
    "                            <option value=\"6\">⚅</option>\n",
    "                        </select>\n",
    "                        <input type=\"checkbox\" id=\"lock2\" class=\"lock-checkbox\">\n",
    "                    </div>\n",
    "                    <div class=\"die-wrapper\">\n",
    "                        <select id=\"die3\" class=\"die-select\">\n",
    "                            <option value=\"\">❓</option>\n",
    "                            <option value=\"1\">⚀</option>\n",
    "                            <option value=\"2\">⚁</option>\n",
    "                            <option value=\"3\">⚂</option>\n",
    "                            <option value=\"4\">⚃</option>\n",
    "                            <option value=\"5\">⚄</option>\n",
    "                            <option value=\"6\">⚅</option>\n",
    "                        </select>\n",
    "                        <input type=\"checkbox\" id=\"lock3\" class=\"lock-checkbox\">\n",
    "                    </div>\n",
    "                    <div class=\"die-wrapper\">\n",
    "                        <select id=\"die4\" class=\"die-select\">\n",
    "                            <option value=\"\">❓</option>\n",
    "                            <option value=\"1\">⚀</option>\n",
    "                            <option value=\"2\">⚁</option>\n",
    "                            <option value=\"3\">⚂</option>\n",
    "                            <option value=\"4\">⚃</option>\n",
    "                            <option value=\"5\">⚄</option>\n",
    "                            <option value=\"6\">⚅</option>\n",
    "                        </select>\n",
    "                        <input type=\"checkbox\" id=\"lock4\" class=\"lock-checkbox\">\n",
    "                    </div>\n",
    "                    <div class=\"die-wrapper\">\n",
    "                        <select id=\"die5\" class=\"die-select\">\n",
    "                            <option value=\"\">❓</option>\n",
    "                            <option value=\"1\">⚀</option>\n",
    "                            <option value=\"2\">⚁</option>\n",
    "                            <option value=\"3\">⚂</option>\n",
    "                            <option value=\"4\">⚃</option>\n",
    "                            <option value=\"5\">⚄</option>\n",
    "                            <option value=\"6\">⚅</option>\n",
    "                        </select>\n",
    "                        <input type=\"checkbox\" id=\"lock5\" class=\"lock-checkbox\">\n",
    "                    </div>\n",
    "                    <div class=\"die-wrapper\">\n",
    "                        <select id=\"die6\" class=\"die-select\">\n",
    "                            <option value=\"\">❓</option>\n",
    "                            <option value=\"1\">⚀</option>\n",
    "                            <option value=\"2\">⚁</option>\n",
    "                            <option value=\"3\">⚂</option>\n",
    "                            <option value=\"4\">⚃</option>\n",
    "                            <option value=\"5\">⚄</option>\n",
    "                            <option value=\"6\">⚅</option>\n",
    "                        </select>\n",
    "                        <input type=\"checkbox\" id=\"lock6\" class=\"lock-checkbox\">\n",
    "                    </div>\n",
    "                </div>\n",
    "            </div>\n",
    "            \n",
    "            <div class=\"controls-row\">\n",
    "                <div class=\"control-group\">\n",
    "                    <div class=\"score-input\">\n",
    "                        <label for=\"locked-score\">Locked Score</label>\n",
    "                        <input type=\"text\" id=\"locked-score\" value=\"\" readonly style=\"background-color: #f0f0f0;\">\n",
    "                    </div>\n",
    "                \n",
    "                    <div class=\"score-input\">\n",
    "                        <label for=\"locked-bust\">Locked Bust</label>\n",
    "                        <input type=\"text\" id=\"locked-bust\" value=\"\" readonly style=\"background-color: #f0f0f0;\">\n",
    "                    </div>\n",
    "                </div>\n",
    "\n",
    "                <div class=\"control-group\">\n",
    "                    <div class=\"score-input\">\n",
    "                        <label for=\"dice-list\">Dice</label>\n",
    "                        <input type=\"text\" id=\"dice-list\" value=\"\" maxlength=\"6\" placeholder=\"000000\">\n",
    "                    </div>\n",
    "                    \n",
    "                    <div class=\"score-input\">\n",
    "                        <label for=\"current-score\">Prev. Score</label>\n",
    "                        <input type=\"number\" id=\"current-score\" value=\"0\" min=\"0\" max=\"9950\" step=\"50\">\n",
    "                    </div>\n",
    "                </div>\n",
    "\n",
    "                <div class=\"control-group button-row\">\n",
    "                    <button id=\"roll-unlocked-btn\" class=\"roll-btn\">Roll Unlocked</button>\n",
    "                    <button id=\"clear-btn\" class=\"clear-btn\">Clear All</button>\n",
    "                </div>\n",
    "            </div>\n",
    "        </div>\n",
    "        \n",
    "        <div class=\"result-section\">\n",
    "            <h3>Optimal Choices</h3>\n",
    "            <table class=\"moves-table\" id=\"moves-table\">\n",
    "                <thead>\n",
    "                    <tr>\n",
    "                        <th>Lock</th>\n",
    "                        <th>Stay?</th>\n",
    "                        <th>Expected Score</th>\n",
    "                        <th>Chance of Bust</th>\n",
    "                    </tr>\n",
    "                </thead>\n",
    "                <tbody id=\"moves-tbody\">\n",
    "                    <tr class=\"placeholder-row\">\n",
    "                        <td colspan=\"4\">Select your dice to see optimal moves...</td>\n",
    "                    </tr>\n",
    "                </tbody>\n",
    "            </table>\n",
    "        </div>\n",
    "    </div>\n",
    "</div>\n",
    "\n",
    "<script>\n",
    "document.addEventListener('DOMContentLoaded', function() {\n",
    "    // Get all dice selects and lock checkboxes\n",
    "    const diceSelects = [1, 2, 3, 4, 5, 6].map(i => document.getElementById(`die${i}`));\n",
    "    const lockCheckboxes = [1, 2, 3, 4, 5, 6].map(i => document.getElementById(`lock${i}`));\n",
    "    const diceListInput = document.getElementById('dice-list');\n",
    "    const currentScoreInput = document.getElementById('current-score');\n",
    "    const lockedScoreInput = document.getElementById('locked-score');\n",
    "    const lockedBustInput = document.getElementById('locked-bust');\n",
    "    const movesTbody = document.getElementById('moves-tbody');\n",
    "    const rollUnlockedBtn = document.getElementById('roll-unlocked-btn');\n",
    "    const clearBtn = document.getElementById('clear-btn');\n",
    "    \n",
    "    let hasData = false; // Track if table has data rows\n",
    "    let farkleData = null; // Store loaded JSON data\n",
    "    let farkleScoresData = null; // Store loaded scores JSON data\n",
    "    let isUpdatingFromDiceList = false; // Prevent circular updates\n",
    "    \n",
    "    // Load the farkle-moves.json file\n",
    "    fetch('farkle-moves.json')\n",
    "        .then(response => response.json())\n",
    "        .then(data => {\n",
    "            farkleData = data;\n",
    "            console.log('Farkle moves data loaded successfully');\n",
    "        })\n",
    "        .catch(error => {\n",
    "            console.error('Error loading farkle moves data:', error);\n",
    "        });\n",
    "\n",
    "    // Load the farkle-scores.json file\n",
    "    fetch('farkle-scores.json')\n",
    "        .then(response => response.json())\n",
    "        .then(data => {\n",
    "            farkleScoresData = data;\n",
    "            console.log('Farkle scores data loaded successfully');\n",
    "            // Trigger update in case dice are already selected\n",
    "            updateCalculation();\n",
    "        })\n",
    "        .catch(error => {\n",
    "            console.error('Error loading farkle scores data:', error);\n",
    "        });\n",
    "    \n",
    "    // Add event listeners to all dice and locks\n",
    "    diceSelects.forEach((select, idx) => {\n",
    "        select.addEventListener('change', () => {\n",
    "            if (!isUpdatingFromDiceList) {\n",
    "                updateDiceListFromSelects();\n",
    "            }\n",
    "            updateCalculation();\n",
    "        });\n",
    "    });\n",
    "    \n",
    "    lockCheckboxes.forEach((checkbox, idx) => {\n",
    "        checkbox.addEventListener('change', () => {\n",
    "            const dieSelect = diceSelects[idx];\n",
    "            if (checkbox.checked) {\n",
    "                dieSelect.classList.add('locked');\n",
    "            } else {\n",
    "                dieSelect.classList.remove('locked');\n",
    "            }\n",
    "            updateCalculation();\n",
    "        });\n",
    "    });\n",
    "    \n",
    "    diceListInput.addEventListener('input', () => {\n",
    "        updateSelectsFromDiceList();\n",
    "    });\n",
    "    \n",
    "    currentScoreInput.addEventListener('input', () => {\n",
    "        // validateAndFixScore();\n",
    "        updateCalculation();\n",
    "    });\n",
    "    \n",
    "    currentScoreInput.addEventListener('blur', () => {\n",
    "        validateAndFixScore();\n",
    "    });\n",
    "    \n",
    "    function updateDiceListFromSelects() {\n",
    "        const diceString = diceSelects.map(s => s.value || '0').join('');\n",
    "        diceListInput.value = diceString;\n",
    "    }\n",
    "    \n",
    "    function updateSelectsFromDiceList() {\n",
    "        const value = diceListInput.value;\n",
    "        \n",
    "        // Validate input - only allow digits 0-6\n",
    "        const sanitized = value.replace(/[^0-6]/g, '');\n",
    "        if (sanitized !== value) {\n",
    "            diceListInput.value = sanitized;\n",
    "            return;\n",
    "        }\n",
    "        \n",
    "        // Pad with zeros if less than 6 digits\n",
    "        const padded = sanitized.padEnd(6, '0').substring(0, 6);\n",
    "        \n",
    "        // Update dice selects\n",
    "        isUpdatingFromDiceList = true;\n",
    "        padded.split('').forEach((digit, idx) => {\n",
    "            if (idx < diceSelects.length) {\n",
    "                diceSelects[idx].value = digit === '0' ? '' : digit;\n",
    "            }\n",
    "        });\n",
    "        isUpdatingFromDiceList = false;\n",
    "        \n",
    "        updateCalculation();\n",
    "    }\n",
    "    \n",
    "    function validateAndFixScore() {\n",
    "        let value = parseInt(currentScoreInput.value) || 0;\n",
    "        \n",
    "        // Ensure it's not negative\n",
    "        if (value < 0) {\n",
    "            value = 0;\n",
    "        }\n",
    "        \n",
    "        // Ensure it's less than 20000\n",
    "        if (value >= 20000) {\n",
    "            value = 19950;\n",
    "        }\n",
    "        \n",
    "        // Round to nearest 50\n",
    "        value = Math.round(value / 50) * 50;\n",
    "        \n",
    "        currentScoreInput.value = value;\n",
    "    }\n",
    "    \n",
    "    // Roll unlocked button functionality\n",
    "    rollUnlockedBtn.addEventListener('click', () => {\n",
    "        // Fill only unlocked dice with random values\n",
    "        diceSelects.forEach((select, idx) => {\n",
    "            if (!lockCheckboxes[idx].checked) {\n",
    "                const randomValue = Math.floor(Math.random() * 6) + 1;\n",
    "                select.value = randomValue;\n",
    "            }\n",
    "        });\n",
    "        \n",
    "        updateDiceListFromSelects();\n",
    "        updateCalculation();\n",
    "    });\n",
    "    \n",
    "    // Clear button functionality\n",
    "    clearBtn.addEventListener('click', () => {\n",
    "        diceSelects.forEach((select, idx) => {\n",
    "            select.value = '';\n",
    "            select.classList.remove('locked');\n",
    "        });\n",
    "        lockCheckboxes.forEach(checkbox => {\n",
    "            checkbox.checked = false;\n",
    "        });\n",
    "        diceListInput.value = '';\n",
    "        currentScoreInput.value = 0;\n",
    "        lockedScoreInput.value = '';\n",
    "        lockedBustInput.value = '';\n",
    "        movesTbody.innerHTML = '<tr class=\"placeholder-row\"><td colspan=\"4\">Select your dice to see optimal moves...</td></tr>';\n",
    "        hasData = false;\n",
    "    });\n",
    "    \n",
    "    function updateCalculation() {\n",
    "        const dice = diceSelects.map(s => s.value ? parseInt(s.value) : null);\n",
    "        const locks = lockCheckboxes.map(cb => cb.checked);\n",
    "        const currentScore = parseInt(currentScoreInput.value) || 0;\n",
    "\n",
    "        // Calculate and update locked stats\n",
    "        updateLockedStats(dice, locks);\n",
    "        \n",
    "        // Check if we have any dice selected\n",
    "        if (!dice.some(d => d !== null)) {\n",
    "            movesTbody.innerHTML = '<tr class=\"placeholder-row\"><td colspan=\"4\">Select your dice to see optimal moves...</td></tr>';\n",
    "            hasData = false;\n",
    "            return;\n",
    "        }\n",
    "        \n",
    "        // Calculate optimal moves\n",
    "        const moves = calculateOptimalMoves(dice, locks, currentScore);\n",
    "        \n",
    "        // Display moves with animation\n",
    "        displayMoves(moves);\n",
    "    }\n",
    "\n",
    "    function updateLockedStats(dice, locks) {\n",
    "        if (!farkleScoresData) {\n",
    "            lockedScoreInput.value = '';\n",
    "            lockedBustInput.value = '';\n",
    "            return;\n",
    "        }\n",
    "\n",
    "        const lockedCounts = [0, 0, 0, 0, 0, 0];\n",
    "        dice.forEach((die, idx) => {\n",
    "            if (die !== null && locks[idx]) {\n",
    "                lockedCounts[die - 1]++;\n",
    "            }\n",
    "        });\n",
    "        \n",
    "        const lockKey = lockedCounts.join('');\n",
    "        \n",
    "        if (lockKey === '000000') {\n",
    "            lockedScoreInput.value = '';\n",
    "            lockedBustInput.value = '';\n",
    "            return;\n",
    "        }\n",
    "        \n",
    "        if (farkleScoresData[lockKey]) {\n",
    "            lockedScoreInput.value = farkleScoresData[lockKey].score;\n",
    "            // Format bust probability as percentage with 1 decimal place\n",
    "            const bustProb = (farkleScoresData[lockKey].bust * 100).toFixed(1);\n",
    "            lockedBustInput.value = bustProb + '%';\n",
    "        } else {\n",
    "            lockedScoreInput.value = '';\n",
    "            lockedBustInput.value = '';\n",
    "        }\n",
    "    }\n",
    "    \n",
    "    function calculateOptimalMoves(dice, locks, currentScore) {\n",
    "        // Check if data is loaded\n",
    "        if (!farkleData) {\n",
    "            return [{\n",
    "                dice: [],\n",
    "                keep: '—',\n",
    "                expectedScore: '—',\n",
    "                chanceOfBust: '—',\n",
    "                error: true,\n",
    "                message: 'Loading data...'\n",
    "            }];\n",
    "        }\n",
    "        \n",
    "        // Count dice by face value (1-6)\n",
    "        const diceCounts = [0, 0, 0, 0, 0, 0];\n",
    "        const lockedCounts = [0, 0, 0, 0, 0, 0];\n",
    "        \n",
    "        dice.forEach((die, idx) => {\n",
    "            if (die !== null) {\n",
    "                diceCounts[die - 1]++;\n",
    "                if (locks[idx]) {\n",
    "                    lockedCounts[die - 1]++;\n",
    "                }\n",
    "            }\n",
    "        });\n",
    "        \n",
    "        // Create hand key: count of each die face (1-6)\n",
    "        const handKey = diceCounts.join('');\n",
    "        \n",
    "        // Create lock key: count of each locked die face (1-6)\n",
    "        const lockKey = lockedCounts.join('');\n",
    "        \n",
    "        // Get the score key (round to nearest 50)\n",
    "        const scoreKey = String(Math.round(currentScore / 50) * 50);\n",
    "        \n",
    "        console.log('Debug:', { handKey, lockKey, scoreKey, dice, locks, currentScore, diceCounts, lockedCounts });\n",
    "        \n",
    "        // Lookup in the JSON\n",
    "        if (!farkleData[handKey]) {\n",
    "            console.log('Hand key not found. Available keys sample:', Object.keys(farkleData).slice(0, 5));\n",
    "            return [{\n",
    "                dice: [],\n",
    "                keep: '—',\n",
    "                expectedScore: '—',\n",
    "                chanceOfBust: '—',\n",
    "                error: true,\n",
    "                message: 'Invalid locks / no valid new locks'\n",
    "            }];\n",
    "        }\n",
    "        \n",
    "        if (!farkleData[handKey][lockKey]) {\n",
    "            console.log('Lock key not found. Available locks:', Object.keys(farkleData[handKey]).slice(0, 5));\n",
    "            return [{\n",
    "                dice: [],\n",
    "                keep: '—',\n",
    "                expectedScore: '—',\n",
    "                chanceOfBust: '—',\n",
    "                error: true,\n",
    "                message: 'Invalid locks / no valid new locks'\n",
    "            }];\n",
    "        }\n",
    "        \n",
    "        if (!farkleData[handKey][lockKey][scoreKey]) {\n",
    "            console.log('Score key not found. Available scores:', Object.keys(farkleData[handKey][lockKey]).slice(0, 10));\n",
    "            return [{\n",
    "                dice: [],\n",
    "                keep: '—',\n",
    "                expectedScore: '—',\n",
    "                chanceOfBust: '—',\n",
    "                error: true,\n",
    "                message: 'Invalid locks / no valid new locks'\n",
    "            }];\n",
    "        }\n",
    "        \n",
    "        // Get the moves from JSON\n",
    "        const jsonMoves = farkleData[handKey][lockKey][scoreKey];\n",
    "        \n",
    "        // Convert to our format\n",
    "        const moves = jsonMoves.map(move => {\n",
    "            // Parse the locked string to get dice to keep\n",
    "            const keepDice = [];\n",
    "            for (let i = 0; i < move.locked.length; i++) {\n",
    "                const count = parseInt(move.locked[i]);\n",
    "                const dieValue = i + 1; // Die faces are 1-6\n",
    "                for (let j = 0; j < count; j++) {\n",
    "                    keepDice.push(dieValue);\n",
    "                }\n",
    "            }\n",
    "            \n",
    "            return {\n",
    "                dice: keepDice,\n",
    "                keep: move.roll ? 'Roll' : 'Bank',\n",
    "                expectedScore: Math.round(move.ev),\n",
    "                chanceOfBust: Math.round(move.bust * 1000) / 10\n",
    "            };\n",
    "        });\n",
    "        \n",
    "        return moves;\n",
    "    }\n",
    "    \n",
    "    function displayMoves(moves) {\n",
    "        const isFirstTime = !hasData;\n",
    "        const hasPlaceholder = movesTbody.querySelector('.placeholder-row') !== null;\n",
    "        \n",
    "        // If transitioning from error/placeholder to data, treat as first time\n",
    "        if (!isFirstTime && hasPlaceholder && moves.length > 0 && !moves[0].error) {\n",
    "            hasData = false;\n",
    "        }\n",
    "        \n",
    "        if (!hasData || hasPlaceholder) {\n",
    "            // First time showing data - fade out placeholder, then show data\n",
    "            const existingRows = movesTbody.querySelectorAll('tr');\n",
    "            existingRows.forEach(row => {\n",
    "                row.classList.add('fade-out');\n",
    "            });\n",
    "            \n",
    "            setTimeout(() => {\n",
    "                createRows(moves);\n",
    "                if (moves.length > 0 && !moves[0].error) {\n",
    "                    hasData = true;\n",
    "                }\n",
    "            }, 200);\n",
    "        } else {\n",
    "            // Update existing rows without collapsing table\n",
    "            updateRows(moves);\n",
    "        }\n",
    "    }\n",
    "    \n",
    "    function createRows(moves) {\n",
    "        movesTbody.innerHTML = '';\n",
    "        \n",
    "        // Check if it's an error message\n",
    "        if (moves.length === 1 && moves[0].error) {\n",
    "            const row = document.createElement('tr');\n",
    "            row.classList.add('placeholder-row');\n",
    "            row.innerHTML = `<td colspan=\"4\">${moves[0].message}</td>`;\n",
    "            movesTbody.appendChild(row);\n",
    "            hasData = false;\n",
    "            return;\n",
    "        }\n",
    "        \n",
    "        moves.forEach((move, idx) => {\n",
    "            setTimeout(() => {\n",
    "                const row = document.createElement('tr');\n",
    "                row.classList.add('move-row', 'fade-in');\n",
    "                \n",
    "                const diceEmojis = move.dice.map(d => getDieEmoji(d)).join(' ');\n",
    "                \n",
    "                row.innerHTML = `\n",
    "                    <td class=\"dice-cell\">${diceEmojis || '—'}</td>\n",
